{
    "name": "stadline/js-extension-bundle",
    "description": "A collection of JS tools",
    "license": "MIT",
    "authors": [
        {
            "name": "alex-pex",
            "email": "alexandre.paixao@stadline.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3",
        "jms/serializer-bundle": "~0.12",
        "dimsemenov/magnific-popup": "dev-master",
        "qtip2/qtip2": "dev-master"
    },
    "suggest": {
        "fxp/composer-asset-plugin": "NPM/Bower Dependency Manager for Composer",
        "bower-asset/jquery": "jQuery JavaScript Library",
        "bower-asset/marionette": "The Backbone framework",
        "bower-asset/magnific-popup": "Light and responsive lightbox script with focus on performance",
        "willdurand/js-translation-bundle": "A pretty nice way to expose your translation messages to your JavaScript",
        "friendsofsymfony/jsrouting-bundle": "A pretty nice way to expose your Symfony2 routing to client applications"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "dimsemenov/magnific-popup",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/dimsemenov/Magnific-Popup.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "qtip2/qtip2",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/qTip2/qTip2",
                    "reference": "master"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "StadLine\\JsExtensionBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
